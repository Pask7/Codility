import java.util.*;

class Solution {
    public int solution(int[] A) {
        int num = 1;
        HashSet<Integer> HSet = new HashSet<Integer>();

        for (int i = 0 ; i < A.length; i++) {
            HSet.add(A[i]);                     
        }

         while (HSet.contains(num)) {
                num++;
            }

        return num;
    }
}
